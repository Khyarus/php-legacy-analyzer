{
  "name": "phpLegacyAnalyzer",
  "displayName": "php-legacy-analyzer",
  "description": "PHP Legacy Analyzer (Clean Code + Complexity)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:php",
    "onCommand:phpLegacyAnalyzer.analyzeProject"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "phpLegacyAnalyzer.analyzeProject",
        "title": "PHP Legacy Analyzer: Analyze Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/node": "22.x",
    "typescript": "^5.9.3",
    "eslint": "^9.39.2"
  }
}
